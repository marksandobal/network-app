<div class="container">
  <h2>Agregar nuevos productos</h2>
  <div>
    <%= form_tag products_path, multipart:true do %>
      <div class="form-group">
        <%= file_field_tag :file %>
      </div>
        
      <div class="form-group">
        <%= submit_tag 'Cargar productos' %>
      </div>
    <% end %>
  </div>
  
</div>